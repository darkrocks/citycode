@{
	this.Layout = this.Request.Browser.IsMobileDevice ? "~/Views/Layouts/_MobileLayout.cshtml" : "~/Views/Layouts/_DesktopLayout.cshtml";
}
@section styles{
	@(this.RenderSection("styles", false))
}

@section scripts{
	@Scripts.Render("~/bundles/jquery-171")
	@Scripts.Render("~/bundles/bootstrap")
	@Scripts.Render("~/bundles/thirdparty")
	@Scripts.Render("~/bundles/guide")
	@(this.RenderSection("scripts", false))
}

@(this.RenderBody())